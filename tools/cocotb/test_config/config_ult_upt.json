{
    "testbench_config": {
        "test_name": "ult_pt",
        "input_args": {
            "n_events": 2,
            "event_detail": false,
            "clock_period": 5,
            "clock_time_unit": "ns",
            "_COMMENT": "Environment variables COMPONENTS_LIB_DIR, L0MDT_TESTVECTOR_DIR overrides components_lib_dir, testvector_dir definition"
        },
        "run_config": {
            "output_directory_name": "ult_upt",
            "test_location": "src/l0mdt_tb/testbench/ult_pt/test",
            "expected_is_observed": false,
            "components_lib_dir": "${XILINX_LIB_DIR}",
            "configure_rtl": true,
            "pt_type": "upt",
            "num_threads": 3
        },
        "testvectors": {
            "testvector_dir": "${L0MDT_TESTVECTOR_DIR}",
            "testvector_file": "B_A_3.xz",
            "inputs": [
                {
                    "tv_format": "SF2PTCALC_LSF",
                    "ports": 3,
                    "station_ID" : ["INN", "INN", "INN"],
                    "thread_n"   : [0,1,2]
                },
                {
                    "tv_format": "SF2PTCALC_LSF",
                    "ports": 3,
                    "station_ID" : ["MID", "MID", "MID"],
                    "thread_n"   : [0,1,2]

                },
                {
                    "tv_format": "SF2PTCALC_LSF",
                    "ports": 3,
                    "station_ID" : ["OUT", "OUT", "OUT"],
                    "thread_n"   : [0,1,2]

                },
                {
                    "tv_format": "PL2PTCALC",
                    "ports": 3,
	            "station_ID" : ["NONE", "NONE", "NONE"],
                    "thread_n"   : [0,1,2]
                }
            ],
            "outputs": [
                {
                    "tv_format": "PTCALC2MTC_LSF",
                    "ports": 3,
	            "station_ID" : ["NONE", "NONE", "NONE"],
                    "thread_n"   : [0,1,2]
                }
            ]
        }
    }
}
